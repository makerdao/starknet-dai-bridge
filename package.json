{
  "name": "starknet-dai-bridge",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:maciejka/starknet-dai-bridge.git",
  "author": "Nick Ulven <nickulven@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --check \"./**/*.ts\"",
    "format:fix": "prettier --write \"./**/*.ts\"",
    "lint": "eslint --ext .ts test scripts hardhat.config.ts",
    "lint:fix": "yarn lint --fix",
    "compile:l1": "hardhat compile",
    "compile:l2": "hardhat starknet-compile",
    "compile": "yarn compile",
    "chain:l1": "hardhat node",
    "chain:l2": "starknet-devnet",
    "chain": "concurrently 'yarn chain:l1' 'yarn chain:l2'",
    "chain:ci": "concurrently 'NODE_ENV=test yarn chain:l1' 'yarn chain:l2'",
    "deploy-deployer:goerli": "STARKNET_NETWORK=alpha-goerli hardhat deploy-deployer --network goerli",
    "deploy-deployer:mainnet": "STARKNET_NETWORK=alpha-mainnet hardhat deploy-deployer --network mainnet",
    "deploy-bridge:goerli": "STARKNET_NETWORK=alpha-goerli hardhat deploy-bridge --network goerli",
    "deploy-bridge:mainnet": "STARKNET_NETWORK=alpha-mainnet hardhat deploy-bridge --network mainnet",
    "deploy-teleport:goerli": "STARKNET_NETWORK=alpha-goerli hardhat deploy-teleport --network goerli",
    "deploy-teleport-spell-l1:goerli": "STARKNET_NETWORK=alpha-goerli hardhat deploy-teleport-spell-l1 --network goerli",
    "deploy-teleport-spell-l2:goerli": "STARKNET_NETWORK=alpha-goerli hardhat deploy-teleport-spell-l2 --network goerli",
    "create-teleport-spell-l1": "hardhat create-teleport-spell-l1",
    "create-teleport-spell-l2": "hardhat create-teleport-spell-l2",
    "run-spell:goerli": "STARKNET_NETWORK=alpha-goerli hardhat run-spell --network goerli",
    "deploy-bridge-upgrade:mainnet": "STARKNET_NETWORK=alpha-mainnet hardhat deploy-bridge-upgrade --network mainnet",
    "deploy-escrow-mom:goerli": "STARKNET_NETWORK=alpha-goerli hardhat deploy-escrow-mom --network goerli",
    "deploy-escrow-mom:mainnet": "STARKNET_NETWORK=alpha-mainnet hardhat deploy-escrow-mom --network mainnet",
    "send-l2-message": "yarn load-messaging-contract && yarn flush",
    "call:l1": "hardhat call:l1",
    "call:l2": "hardhat call:l2",
    "invoke:l2": "hardhat invoke:l2",
    "test:l1": "NODE_ENV=test TEST_ENV=l1 hardhat test",
    "test:e2e": "wait-on tcp:5050 && wait-on tcp:8545 && NODE_ENV=test STARKNET_NETWORK=devnet TEST_ENV=e2e hardhat test --network localhost",
    "test:e2e:ci": "concurrently './scripts/chain.sh' './scripts/test-e2e.sh'",
    "test:l2": "pytest ./test/l2/* --disable-pytest-warnings",
    "test:integration": "NODE_ENV=integration npx hardhat test --network goerli",
    "test:fix": "yarn lint:fix && yarn format:fix && yarn test:l1 && yarn typecheck",
    "inspect-wards:mainnet": "hardhat inspect-wards"
  },
  "devDependencies": {
    "@codechecks/client": "^0.1.11",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@defi-wonderland/smock": "^2.0.7",
    "@dethcrypto/eth-sdk": "^0.3.1",
    "@ethersproject/abi": "^5.4.1",
    "@ethersproject/abstract-signer": "^5.4.1",
    "@ethersproject/bignumber": "^5.4.1",
    "@ethersproject/bytes": "^5.4.0",
    "@ethersproject/providers": "^5.4.5",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@types/chai": "^4.2.21",
    "@types/fs-extra": "^9.0.12",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.1",
    "fs-extra": "^10.0.0",
    "hardhat": "2.8.4",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.2.1",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "shelljs": "^0.8.4",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2"
  },
  "dependencies": {
    "@dethcrypto/eth-sdk-client": "^0.1.6",
    "@eth-optimism/smock": "^1.1.10",
    "@ethersproject/abstract-provider": "^5.4.1",
    "@makerdao/hardhat-utils": "^0.1.3",
    "@nomiclabs/ethereumjs-vm": "^4.2.2",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.6.0",
    "@shardlabs/starknet-hardhat-plugin": "^0.6.2",
    "@types/yargs": "^17.0.7",
    "concurrently": "^7.0.0",
    "earljs": "^0.2.1",
    "eslint-config-typestrict": "^1.0.2",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "keccak256": "^1.0.6",
    "lodash": "^4.17.21",
    "readline": "^1.3.0",
    "starknet": "^2.3.1",
    "ts-essentials": "^9.1.2",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1",
    "yargs": "^17.3.0"
  },
  "resolutions": {
    "hardhat/**/@ethereumjs/tx": "3.5.0"
  }
}
