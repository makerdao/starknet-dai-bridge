{
  "name": "starknet-dai-bridge",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:maciejka/starknet-dai-bridge.git",
  "author": "Nick Ulven <nickulven@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --check \"./**/*.ts\"",
    "format:fix": "prettier --write \"./**/*.ts\"",
    "lint": "eslint --ext .ts test scripts hardhat.config.ts",
    "lint:fix": "yarn lint --fix",
    "compile:l1": "hardhat compile",
    "compile:l2": "hardhat starknet-compile",
    "compile": "yarn compile",
    "chain:l1": "hardhat node",
    "chain:l2": "starknet-devnet --load-path ~/devnet --dump-on exit --dump-path ~/devnet",
    "chain:l2:ci": "starknet-devnet",
    "chain": "concurrently 'yarn chain:l1' 'yarn chain:l2'",
    "chain:ci": "concurrently 'NODE_ENV=test yarn chain:l1' 'yarn chain:l2:ci'",
    "setBalance": "hardhat setBalance --network fork",
    "setDaiBalance": "hardhat setDaiBalance --network fork",
    "deploy-deployer:goerli": "STARKNET_NETWORK=alpha hardhat deploy-deployer --network goerli",
    "deploy-bridge:goerli": "STARKNET_NETWORK=alpha hardhat deploy-bridge --network goerli",
    "deploy-wormhole:goerli": "STARKNET_NETWORK=alpha hardhat deploy-wormhole --network goerli",
    "deploy-deployer:fork": "STARKNET_NETWORK=devnet hardhat deploy-deployer --network fork",
    "deploy-bridge:fork": "STARKNET_NETWORK=devnet hardhat deploy-bridge --network fork",
    "deploy-wormhole:fork": "STARKNET_NETWORK=devnet hardhat deploy-wormhole --network fork",
    "deploy-deployer:fork:goerli": "FORK_NETWORK=goerli yarn deploy-deployer:fork",
    "deploy-deployer:fork:mainnet": "FORK_NETWORK=mainnet yarn deploy-deployer:fork",
    "deploy-bridge:fork:goerli": "FORK_NETWORK=goerli yarn deploy-bridge:fork",
    "deploy-bridge:fork:mainnet": "FORK_NETWORK=mainnet yarn deploy-bridge:fork",
    "deploy-wormhole:fork:goerli": "FORK_NETWORK=goerli yarn deploy-wormhole:fork",
    "deploy-wormhole:fork:mainnet": "FORK_NETWORK=mainnet yarn deploy-wormhole:fork",
    "deploy-wormhole-spell-l1:goerli": "STARKNET_NETWORK=alpha hardhat deploy-wormhole-spell-l1 --network fork",
    "deploy-wormhole-spell-l2:goerli": "STARKNET_NETWORK=alpha hardhat deploy-wormhole-spell-l2 --network goerli",
    "deploy-wormhole-spell-l1:fork": "STARKNET_NETWORK=devnet hardhat deploy-wormhole-spell-l1 --network fork",
    "deploy-wormhole-spell-l1:fork:goerli": "FORK_NETWORK=goerli yarn deploy-wormhole-spell-l1:fork",
    "deploy-wormhole-spell-l1:fork:mainnet": "FORK_NETWORK=mainnet yarn deploy-wormhole-spell-l1:fork",
    "deploy-wormhole-spell-l2:fork": "STARKNET_NETWORK=devnet hardhat deploy-wormhole-spell-l2 --network fork",
    "deploy-wormhole-spell-l2:fork:goerli": "FORK_NETWORK=goerli yarn deploy-wormhole-spell-l2:fork",
    "deploy-wormhole-spell-l2:fork:mainnet": "FORK_NETWORK=mainnet yarn deploy-wormhole-spell-l2:fork",
    "create-wormhole-spell-l1": "hardhat create-wormhole-spell-l1",
    "create-wormhole-spell-l2": "hardhat create-wormhole-spell-l2",
    "run-spell:fork": "FORK_NETWORK=goerli hardhat run-spell --network fork",
    "account:create": "hardhat account:create --network goerli",
    "account:get": "hardhat account:get --network goerli",
    "load-messaging-contract": "FORK_NETWORK=goerli hardhat load-messaging-contract --network fork",
    "flush": "hardhat flush --network fork",
    "send-l2-message": "yarn load-messaging-contract && yarn flush",
    "call:l1": "hardhat call:l1",
    "call:l2": "hardhat call:l2",
    "invoke:l2": "hardhat invoke:l2",
    "test:l1": "NODE_ENV=test TEST_ENV=l1 hardhat test",
    "test:e2e": "wait-on tcp:5000 && wait-on tcp:8545 && TEST_ENV=e2e hardhat test --network localhost",
    "test:e2e:ci": "concurrently './scripts/chain.sh' './scripts/test-e2e.sh'",
    "test:l2": "pytest ./test/l2/* --disable-pytest-warnings",
    "test:fix": "yarn lint:fix && yarn format:fix && yarn test:l1 && yarn typecheck"
  },
  "devDependencies": {
    "@codechecks/client": "^0.1.11",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@defi-wonderland/smock": "^2.0.7",
    "@dethcrypto/eth-sdk": "^0.3.1",
    "@ethersproject/abi": "^5.4.1",
    "@ethersproject/abstract-signer": "^5.4.1",
    "@ethersproject/bignumber": "^5.4.1",
    "@ethersproject/bytes": "^5.4.0",
    "@ethersproject/providers": "^5.4.5",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@types/chai": "^4.2.21",
    "@types/fs-extra": "^9.0.12",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.1",
    "fs-extra": "^10.0.0",
    "hardhat": "2.8.4",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.2.1",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "shelljs": "^0.8.4",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2"
  },
  "dependencies": {
    "@dethcrypto/eth-sdk-client": "^0.1.6",
    "@eth-optimism/smock": "^1.1.10",
    "@ethersproject/abstract-provider": "^5.4.1",
    "@makerdao/hardhat-utils": "^0.1.3",
    "@nomiclabs/ethereumjs-vm": "^4.2.2",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "3.4.2-solc-0.7",
    "@shardlabs/starknet-hardhat-plugin": "0.5.2",
    "@types/yargs": "^17.0.7",
    "concurrently": "^7.0.0",
    "earljs": "^0.2.1",
    "eslint-config-typestrict": "^1.0.2",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "keccak256": "^1.0.6",
    "lodash": "^4.17.21",
    "starknet": "^2.3.1",
    "ts-essentials": "^9.1.2",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1",
    "yargs": "^17.3.0"
  },
  "resolutions": {
    "hardhat/**/@ethereumjs/tx": "3.5.0"
  }
}
