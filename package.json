  "scripts": {
    "format": "prettier --check \"./**/*.ts\"",
    "format:fix": "prettier --write \"./**/*.ts\"",
    "lint": "eslint --ext .ts test scripts hardhat.config.ts",
    "lint:fix": "yarn lint --fix",
    "compile:l1": "hardhat compile",
    "deploy:goerli": "hardhat run --network goerli ./scripts/deployGoerli.ts",
    "deploy:l2:bridge": "python ./scripts/deploy.py l2_dai_bridge",
    "deploy:l2:dai": "python ./scripts/deploy.py dai",
    "deploy:l2:account": "python ./scripts/deploy.py Account",
    "interact:l2:bridge": "python ./scripts/interact.py l2_dai_bridge",
    "interact:l2:dai": "python ./scripts/interact.py dai",
    "init-l2-bridge": "python ./scripts/initialize.py",
    "test:l1": "hardhat test",
    "test:l2": "pytest ./test/l2/test.py --disable-pytest-warnings",
    "test:fix": "yarn lint:fix && yarn format:fix && yarn test:l1 && yarn typecheck"
  },
  "devDependencies": {
    "@codechecks/client": "^0.1.11",
